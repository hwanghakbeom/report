<div class="container">
	<div class="container">
		<div class="row">
			<div class="area-top clearfix">
				<div class="pull-left header">
					<h3 class="title">
						Report List
					</h3>
					<h5>
						<span>
							Report List
						</span>
					</h5>
				</div>
			</div>
		</div>

		<div class="container">
			<div class="box">
				<div class="box-header">
					<div class="title">Filter</div>
				</div>
				<div class="box-content">
					<table class="table">
						<tbody>
							<tr>
								<td>App Version</td>
								<td class="in-checkbox">
									<input type="checkbox" class="icheck" checked id="icheck1">
									<label for="icheck1">All</label>
								</td>
								<td class="in-checkbox">
									<input type="checkbox" class="icheck" id="icheck2">
									<label for="icheck2">0.1</label>
								</td>
								<td class="in-checkbox">
									<input type="checkbox" class="icheck" id="icheck3">
									<label for="icheck2">1.0</label>
								</td>
								<td class="in-checkbox">
									<input type="checkbox" class="icheck" id="icheck4">
									<label for="icheck2">2.5</label>
								</td>
							</tr>
							<tr>
								<td>OS Version</td>
								<td class="in-checkbox">
									<input type="checkbox" class="icheck" checked id="icheck1">
									<label for="icheck1">All</label>
								</td>
								<td class="in-checkbox">
									<input type="checkbox" class="icheck" id="icheck2">
									<label for="icheck2">0.1</label>
								</td>
								<td class="in-checkbox">
									<input type="checkbox" class="icheck" id="icheck3">
									<label for="icheck2">1.0</label>
								</td>
								<td class="in-checkbox">
									<input type="checkbox" class="icheck" id="icheck4">
									<label for="icheck2">2.5</label>
								</td>
							</tr>
							<tr>
								<td>Category</td>
								<td class="in-checkbox">
									<input type="checkbox" class="icheck" checked id="icheck1">
									<label for="icheck1">All</label>
								</td>
								<td class="in-checkbox">
									<input type="checkbox" class="icheck" id="icheck2">
									<label for="icheck2">Memory</label>
								</td>
								<td class="in-checkbox">
									<input type="checkbox" class="icheck" id="icheck3">
									<label for="icheck2">CPU</label>
								</td>
								<td class="in-checkbox">
									<input type="checkbox" class="icheck" id="icheck3">
									<label for="icheck2">Network</label>
								</td>
								<td class="in-checkbox">
									<input type="checkbox" class="icheck" id="icheck3">
									<label for="icheck2">Battery</label>
								</td>
							</tr>
							<tr>
								<td>Scenario Test</td>
								<td class="in-checkbox">
									<input type="radio" name="optionsRadios[test]" class="icheck" checked id="iradio1">
                  <label for="iradio1">Yes</label>
								</td>
								<td class="in-checkbox">
									<input type="radio" name="optionsRadios[test]" class="icheck" id="iradio2">
                  <label for="iradio2">No</label>
								</td>
							</tr>
							<tr>
								<td>Error Occurred</td>
								<td class="in-checkbox">
									<input type="radio" name="optionsRadios[error]" class="icheck" id="iradio1">
                  <label for="iradio3">Yes</label>
								</td>
								<td class="in-checkbox">
									<input type="radio" name="optionsRadios[error]" class="icheck" checked id="iradio4">
                  <label for="iradio2">No</label>
								</td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
		</div>

		<div class="container">
			<div class="box">
				<div class="box-content">
					<table id="dataList" class="table table-normal">
						<thead>
							<tr>
								<td></td>
								<td>App version</td>
								<td>Category(icon)</td>
								<td>Status</td>
								<td>Time for Profiling</td>
								<td>Date</td>
							</tr>
						</thead>
						<tbody>
							<% @reports.each do |report| %>
								<tr>
									<td></td>
									<td><%= link_to report.appversion, [@project, report] %></td>
									<td><%= report.categories %></td>
									<td>Status</td>
									<td><%= report.time_for_profiling %></td>
									<td><%= report.created_at_date %></td>
								</tr>
							<% end  %>
						</tbody>
					</table>
				</div>
			</div>
			<div class="text-center">
				<ul class="pagination">
					<li><a href="#">Prev</a></li>
					<li><a href="#">1</a></li>
					<li><a href="#">2</a></li>
					<li><a href="#">3</a></li>
					<li><a href="#">4</a></li>
					<li><a href="#">Next</a></li>
				</ul>
			</div>
		</div>
	</div>
</div>